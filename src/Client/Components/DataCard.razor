<div class="card data-card" style="@customCss">
    @foreach (KeyValuePair<string, string> entry in Entries)
    {
        <div class="entry">
            <p class="label">@entry.Key</p>
            <p class="value" title="@entry.Value">@entry.Value</p>
            @*<i class="fa-solid fa-xmark methodexecutor" onclick=""></i>*@
        </div>
        @if(OnClick.HasDelegate){
            <button class="fa-solid fa-xmark methodexecutor" @onclick="@(() => OnClick.InvokeAsync())"></button>
        }
    }
</div>

@code {
    [Parameter] public EventCallback<string> OnClick { get; set; } = new();

    [Parameter] public string customCss { get; set; } = "";
}


